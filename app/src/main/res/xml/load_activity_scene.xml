<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="2000"
        motion:autoTransition="animateToEnd">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_marginEnd="12dp"
            android:layout_height="363dp"
            android:layout_marginRight="12dp"
            android:layout_marginBottom="232dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="376dp"
            android:id="@+id/imageView6"
            motion:layout_editor_absoluteY="136dp" />
        <Constraint
            android:layout_marginEnd="16dp"
            android:layout_height="643dp"
            android:layout_marginRight="16dp"
            android:layout_marginBottom="56dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="341dp"
            android:id="@+id/imageView5" />
        <Constraint
            android:id="@+id/txt_create_splash"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:visibility="invisible"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/imageView6" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/textView4"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="@+id/txt_create_splash"
            motion:layout_constraintVertical_bias="0.25"
            android:visibility="invisible" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/imageView6"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="318dp"
            android:layout_height="320dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="271dp"
            android:layout_marginEnd="55dp"
            android:rotation="180"
            android:visibility="invisible" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/txt_create_splash"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/imageView6"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.49" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="@+id/txt_create_splash"
            android:layout_width="wrap_content"
            android:id="@+id/textView4"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintVertical_bias="0.24000001" />

    </ConstraintSet>
</MotionScene>